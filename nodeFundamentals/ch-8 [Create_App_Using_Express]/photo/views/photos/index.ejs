<!DOCTYPE html>
    <html>
        <head>
            <title><%= title %></title>
                <link rel='stylesheet' href='/stylesheets/style.css' />
            </head>
                <body>
                    <h1>Photos <%=settings.title%></h1>
                    <p>
					Welcome to <%= title %>
					&nbsp;&nbsp;&nbsp;<a href="/upload">Upload more pics</a>
					</p>
                        <div id="photos">
						
                            <% photos.forEach(function(photo){%>
                                <div class="photo">
                                <h2><%=photo.name%></h2>
                                <img src='/photos/<%=photo.path%>' />
                                </div>
                                <%})%>
                                </div>
								
								
                                </body>
                            </html>